{
  "ts-node": {
    "swc": true
  },
  "compilerOptions": {
    "target": "es2020",
    "lib": [
      "es2020"
    ],
    "rootDir": "./src",
    "outDir": "./dist",
    "module": "commonjs",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strict": true,
    "types": [
      "node",
      "vitest"
    ],
    "noEmit": false
  }
}
